{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "project": "./tsconfig.json"
  },
  "plugins": ["@typescript-eslint"],
  "extends": ["plugin:@typescript-eslint/recommended"],
  "ignorePatterns": ["concept/", "node_modules/", "dist/"],
  "rules": {
      "camelcase": "off",
      "@typescript-eslint/camelcase": "off"
  }
}